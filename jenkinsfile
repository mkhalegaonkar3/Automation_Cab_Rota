pipeline{
    agent any
    
    stages{
        stage('checkout'){
            steps{
                git 'https://github.com/mkhalegaonkar3/product-service-go.git'
            }
        }
        stage ('build') {
            steps {
              bat 'echo "building..."'
            }
        }    
        stage ('test') {
            steps {
              bat 'echo "testing..."'
            }
        }    
        stage ('deploy') {
            steps {
              bat 'echo "deploying..."'
            }
        }
    }
}
